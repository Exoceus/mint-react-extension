{
  "name": "Mint Chrome Extension",
  "version": "1.0",
  "description": "Productivity centric homepage",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "management",
    "history",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"
}